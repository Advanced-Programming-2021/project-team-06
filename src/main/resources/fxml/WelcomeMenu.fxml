<?import javafx.scene.layout.StackPane?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="stackPane" prefHeight="680.0" prefWidth="1200.0"
           xmlns="http://javafx.com/javafx/16" fx:controller="view.graphics.menus.WelcomeMenuView">
    <AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx">


        <Button fx:id="registerButton" layoutX="570.0" layoutY="327.0" onMouseClicked="#openRegisterPage"
                prefHeight="30" prefWidth="100" text="Register" BorderPane.alignment="CENTER"/>

        <Button layoutX="87.0" layoutY="568.0" onMouseClicked="#exitClicked" text="Exit" BorderPane.alignment="CENTER"/>

        <Button fx:id="loginButton" layoutX="597.0" layoutY="392.0" onMouseClicked="#openLoginPage" text="Login"
                BorderPane.alignment="CENTER"/>
        <Label layoutX="595.0" layoutY="35.0" text="Yu-Gi-Oh" BorderPane.alignment="CENTER"/>


    </AnchorPane>
</StackPane>
