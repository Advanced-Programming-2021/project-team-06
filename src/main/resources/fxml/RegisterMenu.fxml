<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="680.0" prefWidth="1200.0"
            xmlns="http://javafx.com/javafx/16" fx:controller="view.graphics.menus.RegisterMenuView">

    <Label fx:id="LoginLabel" layoutX="580.0" layoutY="92.0" text="Register Menu"/>

    <VBox alignment="CENTER" layoutX="492.0" layoutY="251.0" spacing="10">
        <TextField fx:id="registerUsernameInput" maxHeight="70" maxWidth="335" prefHeight="25.0" prefWidth="239.0"/>
        <TextField fx:id="registerNicknameInput" maxHeight="70" maxWidth="335" prefHeight="25.0" prefWidth="239.0"/>
        <TextField fx:id="registerPasswordInput" maxHeight="70" maxWidth="335"/>
    </VBox>
    <Button fx:id="backButton" layoutX="103.0" layoutY="527.0" onMouseClicked="#openFirstPage" text="Back"/>
    <Label layoutX="430.0" layoutY="251.0" text="Username"/>
    <Label layoutX="432.0" layoutY="323.0" text="Password"/>
    <Label layoutX="430.0" layoutY="291.0" prefHeight="17.0" prefWidth="54.0" text="Nickname"/>
    <Button layoutX="580.0" layoutY="369.0" onMouseClicked="#registerUser"
            text="Register!"/>

</AnchorPane>
